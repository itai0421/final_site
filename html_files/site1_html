            if ($('.footer_list').is(':visible')) {
                $(".footer_list").slideUp(300);
            }
            if ($(this).next(".footer_list").is(':visible')) {
                $(this).next(".footer_list").slideUp(300);
            } else {
                $(this).next(".footer_list").slideDown(300);

            }
        });
        }
